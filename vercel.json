{
  "version": 2,
  "builds": [
    {
      "src": "BackEnd/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "FrontEnd/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/BackEnd/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/FrontEnd/$1"
    }
  ]
}